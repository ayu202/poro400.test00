FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

D 0;
Fc 0.5;
Ft 0.5;

porosity1
{
    type            explicitPorositySource;
    active          true;
    selectionMode   cellZone;
    cellZone        line0;

    explicitPorositySourceCoeffs
    {
        selectionMode   cellZone;
        
        cellZone        line0;
        
        type            DarcyForchheimer;

        d   ($D $D $D);
        f   ($Fc $Fc $Fc);

        coordinateSystem
        {
            origin  (0 0 0);

            rotation
            {
                type    none;
            }
        }
    }
}


